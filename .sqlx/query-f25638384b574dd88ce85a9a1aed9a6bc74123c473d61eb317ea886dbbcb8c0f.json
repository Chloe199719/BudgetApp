  "query": "\n            UPDATE categories\n            SET\n                category_name = COALESCE($1, category_name),\n                description = COALESCE($2, description)\n            WHERE category_id = $3 AND user_id = $4 AND (\n                $1 IS DISTINCT FROM category_name OR\n                $2 IS DISTINCT FROM description\n            )\n            RETURNING *\n        ",
